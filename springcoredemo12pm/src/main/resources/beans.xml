<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    
    <bean class= "com.di.Product" id = "prod">
    
    <constructor-arg value = "101"/>
    <constructor-arg value = "Phone"/>
    <constructor-arg value = "1011"/>
    
    </bean>
    
    <bean class = "com.di.Car" id = "car">
    
    <property name="color" value ="Red"/>
    <property name="Price" value ="1011"/>
    
    </bean>
    
    <bean class = "com.di.Employee" id = "emp">
    
    <property name = "id" value = "102" />
    <property name="name" value = "Piyush"/>
    <property name="salary" value = "1200"/>
    <property name="car" ref="car"/>
    
    
    </bean>
    

</beans>